function [up_data, down_data, smooth_up_data, smooth_down_data] = Acquire_saved_signal(app)
% Calls back the txt file from a given pathname and filename. Function is
% called by LoadDataButtonPushed

    all_data = table2array(readtable(app.saved));

    up_data = all_data(1:1000);
    down_data = all_data(1001:2000);
    smooth_up_data = all_data(2001:3000);
    smooth_down_data = all_data(3001:4000);

    
end